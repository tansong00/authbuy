<section id="mid">
  <div class="container mid_cont pad8">
    <div class="row-fluid w-product">
      <div class="cs-boxcontent clearfix">
        <div class="pfimg"><img src="/images/cs-mbe.jpg" style="border: 0 solid; float: right; margin-right: -40px;" class="folioimgmargin" alt="MBE Education"></div>
        <div class="cs-left">
          <h5>MBE Education</h5>
          <div class="dotted-break1"></div>
          <p>MBE Education is a business that brings together the best business minds in the country to teach business owners how to maximise the value of their business. The team at MBE approached Locomote to re-develop their existing website into a more engaging online presence and to increase their conversion rate.</p>
          <div class="dotted-break2"></div>
          <img alt="" src="/images/web-dev_small.jpg" class="cs-icons"><img alt="" src="/images/digital-strategy-small.jpg" class="cs-icons"> <a href="#" class="view-more1 fltrt"><span>View More</span></a>
          <div class="clear"></div>
          <div class="dotted-break2"></div>
        </div>
        <div class="clear"></div>
      </div>
      <div class="row innerpad1 h_cont pad7 tab1 product-list">
        <div class="trans hover" id="backBoard"></div>
        <% @products.each do |product| %>
        <dl class="proItem">
          <dt><a href="products/<%= product.id %>"> <%= image_tag product.photo.url(:middle) %></a></dt>
          <dd class="red">Price: <em>US</em> $<%= product.price%></dd>
          <dd class="lightGray"><%= product.created_at.strftime("%y-%m-%d") %></dd>
        </dl>

        <% end %>

      </div>
      <div class="pagination pagination-right innerpad1">
          <%= paginate @products %>
      </div>
    </div>
  </div>
</section>