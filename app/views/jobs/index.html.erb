<section id="mid">
  <div class="container mid_cont h_cont">
    <div class="row-fluid">
      <div class="job-list">
        <table class="table table-hover" style="text-align: center">
          <% @jobs.each do |job| %>
          <tr style="text-align: center"><td><%= job.title %></td><td><%= job.location %></td><td><%= job.created_at %></td></tr>
          <% end %>
        </table>
      </div>


    </div>
  </div>
</section>